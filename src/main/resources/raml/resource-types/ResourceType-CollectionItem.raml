    200:
      body:
        application/json:
          type: <<responseDelete>>
          example: <<responseDeleteExample>>
    500:
      body:
        application/json:
          type: <<errorObject>>
          example: !include ../examples/Example-Error500.json
    400:
      body:
        application/json:
          type: <<errorObject>>
          example: !include ../examples/Example-Error400.json
    401:
      body:
        application/json:
          type: <<errorObject>>
          example: !include ../examples/Example-Error401.json
    404:
      body:
        application/json:
          type: <<errorObject>>
          example: !include ../examples/Example-Error404.json